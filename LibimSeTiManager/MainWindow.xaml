<Window x:Class="LibimSeTiManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LibimSeTiManager"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="207,44,0,0" VerticalAlignment="Top" Width="75" Click="button_Click" Grid.Column="1"/>
        <TextBox x:Name="logBox" Height="120" Margin="0,200,0,0" TextWrapping="Wrap" IsReadOnly="True" Text="{Binding Log}" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Width="507"/>


    </Grid>
</Window>
